{"ast":null,"code":"import { defineComponent } from 'vue';\nimport { CountUp } from 'countup.js';\nexport default defineComponent({\n  name: 'AttendanceView',\n  data() {\n    return {\n      count: 0,\n      numreg: 399,\n      numontime: 121,\n      currentDate: null,\n      currentTime: null\n    };\n  },\n  methods: {},\n  mounted() {\n    const options = {\n      duration: 3,\n      separator: ','\n    };\n    const numRegCountUp = new CountUp('numreg', this.numreg, options);\n    numRegCountUp.start();\n    const numOnTimeCountUp = new CountUp('numontime', this.numontime, options);\n    numOnTimeCountUp.start();\n\n    //setting up the date and time\n\n    const now = new Date();\n    this.currentTime = now.toLocaleTimeString();\n    this.currentDate = now.toLocaleDateString();\n    console.log(this.currentTime);\n  }\n});","map":{"version":3,"names":["defineComponent","CountUp","name","data","count","numreg","numontime","currentDate","currentTime","methods","mounted","options","duration","separator","numRegCountUp","start","numOnTimeCountUp","now","Date","toLocaleTimeString","toLocaleDateString","console","log"],"sources":["/Users/hardpro/Desktop/Vue JS/aics-attend/src/views/Admin/AttendanceView.vue"],"sourcesContent":["<template>\n\n<div class=\"container-fluid pt-4 px-4\">\n                <div class=\"row g-4\">\n                    <div class=\"col-sm-12 col-xl-4\">\n                        <div class=\"bg-light rounded d-flex align-items-center justify-content-between p-4\">\n                            <i class=\"fa fa-calendar fa-3x text-primary\"></i>\n                            <div class=\"ms-3\">\n                                <h4 class=\"mb-2\">Today</h4>\n                                <h3 class=\"mb-0 text-primary\">{{ currentDate }}</h3>\n                            </div>\n                        </div>\n                        \n                    </div>\n                    <div class=\"col-sm-12 col-md-6 col-xl-4\">\n                        <div class=\"bg-light rounded d-flex align-items-center justify-content-between p-4\">\n                            <i class=\"fa fa-check fa-3x text-primary\"></i>\n                            <div class=\"ms-3\">\n                                <h4 class=\"mb-2\">Check-In</h4>\n                                <h3 class=\"mb-0 text-primary\" id=\"numreg\">{{ numactive }}</h3>\n                            </div>\n                        </div>\n                        \n                    </div>\n                    <div class=\"col-sm-12 col-md-6 col-xl-4\">\n                        <div class=\"bg-light rounded d-flex align-items-center justify-content-between p-4\">\n                            <i class=\"fa fa-clock fa-3x text-primary\"></i>\n                            <div class=\"ms-3\">\n                                <h4 class=\"mb-2\">On-time</h4>\n                                <h3 class=\"mb-0 text-primary\" id=\"numontime\">{{ numontime }}</h3>\n                            </div>\n                        </div>\n                        \n                    </div>\n                    \n                </div>\n        </div>\n    <div class=\"container-fluid pt-4 px-4\">\n                <div class=\"bg-light text-center rounded p-4 custom-row\">\n                    <div class=\"d-flex align-items-center justify-content-between mb-4 custom\">\n                        <h5 class=\"mb-0 text-primary\">Fingerprint Attendance</h5>\n                        <button class=\"btn btn-success\">Start</button>\n                        \n                    </div>\n                    <div class=\"scan\">\n                        <div class=\"fingerprint\"></div>\n                        <h3>Scanning...</h3>\n\n                    </div>\n\n                    <!-- <div class=\"finger\">\n                        \n                        <img src=\"../../assets/attendance.png\" id=\"img\"/>\n                        <h3>Please place your finger on the scanner <i class=\"fa fa-arrow-right\"></i></h3>\n                    </div> -->\n                    \n                </div>\n            </div>\n  \n</template>\n\n<script>\nimport { defineComponent } from 'vue'\nimport { CountUp } from 'countup.js';\nexport default defineComponent({\n    name:'AttendanceView',\n    data(){\n        return{\n            count:0,\n            numreg:399,\n            numontime:121,\n            currentDate:null,\n            currentTime:null\n        }\n    },\n    methods:{\n\n    },\n    mounted(){\n        const options = {\n        duration: 3, \n        separator: ',',\n        };\n        const numRegCountUp = new CountUp('numreg', this.numreg,options); \n        numRegCountUp.start(); \n\n        const numOnTimeCountUp = new CountUp('numontime',this.numontime,options); \n        numOnTimeCountUp.start(); \n\n        //setting up the date and time\n\n        const now = new Date()\n        this.currentTime = now.toLocaleTimeString()\n        this.currentDate = now.toLocaleDateString()\n        console.log(this.currentTime)\n\n\n\n    }\n\n\n})\n</script>\n\n<style>\n#search{\n    width: 45%;\n    border-radius: 17px;\n}\n.finger{\n    position: relative;\n    width:100%;\n    display: flex;\n    flex-direction: column;\n    align-content: center;\n    justify-content: center;\n    \n}\n\n@keyframes clip {\n    0% {\n    opacity: 0;\n  }\n  50% {\n    opacity: 1;\n  }\n  100% {\n    opacity: 0;\n  }\n}\n.finger img{\n    position: relative;\n    width:40%;\n    height: 100%;\n    margin: 0 auto;\n   \n    animation:clip  3s ease-in-out infinite;\n        \n}\n\n.finger h3{\n    margin-top: 5px;\n}\n/*fingerprint animation scan */\n.scan{\n    position: relative;\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    width: 100%;\n    height: 300px;\n}\n.scan .fingerprint{\n    position: relative;\n    width: 100%;\n    height: 100%;\n    background: url('../../assets/attendance2.png');\n\n}\n\n\n\n\n/* media and screen sizes */\n@media screen and (max-width: 610px) {\n    .custom{\n        display: flex;\n        flex-direction: column;\n    }\n    .column #search{\n        margin-top: 2px;\n        margin-bottom: 2px;\n        width:80%;\n    }\n    .column div button{\n        margin-top: 3px;\n\n    }\n\n    \n}\n\n</style>"],"mappings":"AA8DA,SAASA,eAAc,QAAS,KAAI;AACpC,SAASC,OAAM,QAAS,YAAY;AACpC,eAAeD,eAAe,CAAC;EAC3BE,IAAI,EAAC,gBAAgB;EACrBC,IAAIA,CAAA,EAAE;IACF,OAAM;MACFC,KAAK,EAAC,CAAC;MACPC,MAAM,EAAC,GAAG;MACVC,SAAS,EAAC,GAAG;MACbC,WAAW,EAAC,IAAI;MAChBC,WAAW,EAAC;IAChB;EACJ,CAAC;EACDC,OAAO,EAAC,CAER,CAAC;EACDC,OAAOA,CAAA,EAAE;IACL,MAAMC,OAAM,GAAI;MAChBC,QAAQ,EAAE,CAAC;MACXC,SAAS,EAAE;IACX,CAAC;IACD,MAAMC,aAAY,GAAI,IAAIb,OAAO,CAAC,QAAQ,EAAE,IAAI,CAACI,MAAM,EAACM,OAAO,CAAC;IAChEG,aAAa,CAACC,KAAK,CAAC,CAAC;IAErB,MAAMC,gBAAe,GAAI,IAAIf,OAAO,CAAC,WAAW,EAAC,IAAI,CAACK,SAAS,EAACK,OAAO,CAAC;IACxEK,gBAAgB,CAACD,KAAK,CAAC,CAAC;;IAExB;;IAEA,MAAME,GAAE,GAAI,IAAIC,IAAI,CAAC;IACrB,IAAI,CAACV,WAAU,GAAIS,GAAG,CAACE,kBAAkB,CAAC;IAC1C,IAAI,CAACZ,WAAU,GAAIU,GAAG,CAACG,kBAAkB,CAAC;IAC1CC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACd,WAAW;EAIhC;AAGJ,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}