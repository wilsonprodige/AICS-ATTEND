{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport Time from '../src/components/Time.vue';\nimport $ from 'jquery';\nimport SidebarMixin from './SidebarMixin.js';\nimport { mapActions, mapGetters } from 'vuex';\nimport { makeRequest } from './axios';\nexport default {\n  mixins: [SidebarMixin],\n  components: {\n    Time\n  },\n  data() {\n    return {\n      viewpwd: false,\n      isLoaded: false,\n      isopen: false,\n      profileImage: true,\n      username: 'John',\n      email: 'john@gmail.com',\n      phone: '672369066',\n      password: 'John1234#',\n      account: 'Admin',\n      profileAvatar: 'profile2.jpg',\n      getUser: [],\n      privs: []\n    };\n  },\n  mounted() {\n    this.isLoaded = true;\n    if (SidebarMixin.ShouldShowSidebar) {\n      //spinner.classList.remove('show')\n      // const script = document.createElement('script')\n      // script.src = require('./assets/loaded.js')\n      // document.head.appendChild(script)\n\n      //profile check\n      if (!this.profileAvatar) {\n        this.profileImage = false;\n      }\n      console.log('mounted');\n      //console.log(this.getUId)\n      //console.log(this.getAuthStatus)\n      //fetch user personal data\n\n      //fetch user role\n\n      //check if user is authenticated\n    }\n  },\n\n  beforeCreate() {\n    console.log(\"before create\");\n  },\n  beforeMount() {\n    console.log(\"before mount\");\n  },\n  created() {\n    // if (this.isAuthenticated) {\n    //   this.fetchprivs();\n    if (SidebarMixin.ShouldShowSidebar) {\n      if (this.isAuthenticated) {\n        this.fetch_user_data();\n      } else {\n        this.$router.push('/');\n      }\n    }\n  },\n  computed: {\n    ...mapGetters(['getUId', 'getAuthStatus']),\n    isAuthenticated() {\n      return this.getAuthStatus;\n    }\n  },\n  methods: {\n    ...mapActions(['setUId', 'setAuth']),\n    toggle() {\n      $('.sidebar, .content').toggleClass(\"open\");\n      return false;\n    },\n    profileChanged(event) {\n      this.profileImage = false;\n      this.profileAvatar = event.target.files[0];\n    },\n    viewPassword() {\n      const pwd = document.getElementById('pwd');\n      if (pwd.type === 'password') {\n        pwd.type = 'text';\n      } else {\n        pwd.type = 'password';\n      }\n      this.viewpwd = !this.viewpwd;\n    },\n    logout() {\n      makeRequest.get('/auth/logout').then(res => {\n        if (!res.data.Error) {\n          //set data in the store null\n          const user_id = null;\n          const authStatus = false;\n          this.setUId(user_id);\n          this.setAuth(authStatus);\n          this.$router.push('/');\n          console.log(\"logged out\");\n        } else {\n          this.$toast.error(res.data.Error);\n        }\n      }).catch(err => {\n        console.log(err);\n        this.$toast.error(\"Network error, please try again\");\n      });\n    },\n    fetch_user_data() {\n      makeRequest.get('/auth/').then(res => {\n        if (!res.data.Error) {\n          this.getUser = res.data;\n          console.log(res.data);\n          this.fetchprivs(res.data.roleId);\n        } else {\n          console.log(res.data.Eror);\n        }\n      }).catch(err => {\n        console.log(err);\n        this.$toast(\"could not load user details\");\n      });\n    },\n    fetchprivs(role_id) {\n      let user_role = role_id;\n      makeRequest.get(\"/roles/getprivs/\" + user_role).then(result => {\n        if (!result.data.Error) {\n          console.log(result.data.Privileges);\n          this.privs = result.data.Privileges;\n        } else {\n          this.$toast.error(this.data.error);\n        }\n      }).catch(error => {\n        this.$toast.error(\"Network error, try agin\");\n        console.log(error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["Time","$","SidebarMixin","mapActions","mapGetters","makeRequest","mixins","components","data","viewpwd","isLoaded","isopen","profileImage","username","email","phone","password","account","profileAvatar","getUser","privs","mounted","ShouldShowSidebar","console","log","beforeCreate","beforeMount","created","isAuthenticated","fetch_user_data","$router","push","computed","getAuthStatus","methods","toggle","toggleClass","profileChanged","event","target","files","viewPassword","pwd","document","getElementById","type","logout","get","then","res","Error","user_id","authStatus","setUId","setAuth","$toast","error","catch","err","fetchprivs","roleId","Eror","role_id","user_role","result","Privileges"],"sources":["/Users/hardpro/Desktop/Vue JS/aics-attend/client/src/App.vue"],"sourcesContent":["<template>\n  <!-- <nav>\n    <router-link to=\"/\">Home</router-link> |\n    <router-link to=\"/about\">About</router-link>\n  </nav> -->\n\n  \n  <div class=\"container-xxl position-relative bg-white d-flex p-0\" v-if=\"ShouldShowSidebar\">\n\n        <!-- Spinner Start -->\n        <div v-if=\"!isLoaded\" id=\"spinner\" class=\"show bg-white position-fixed translate-middle w-100 vh-100 top-50 start-50 d-flex align-items-center justify-content-center\">\n            <div class=\"spinner-border text-primary\" style=\"width: 3rem; height: 3rem;\" role=\"status\">\n                <span class=\"sr-only\">Loading...</span>\n            </div>\n        </div>\n        <!-- Spinner End -->\n\n\n        <!-- Sidebar Start -->\n        <div class=\"sidebar pe-4 pb-3\">\n            <nav class=\"navbar bg-light navbar-light\">\n                <a href=\"#\" class=\"navbar-brand brand-details mx-5 mb-3\">\n                    <!-- <h3 class=\"text-primary\"><span class=\"badge bg-primary\">A</span>ICS-ATTEND</h3> -->\n                    <h2 class=\"text-primary fw-bold\"><b>A</b></h2><span class=\"title\"><p class=\"text-underline-primary\">ICS-ATTEND</p></span>\n\n                </a>\n                <div class=\"d-flex align-items-center ms-4 mb-4 user-details\">\n                    <div class=\"position-relative\">\n                        <img class=\"rounded-circle\" src=\"./assets/profile2.jpg\" alt=\"\" style=\"width: 40px; height: 40px;\">\n                        <div class=\"bg-success rounded-circle border border-2 border-white position-absolute end-0 bottom-0 p-1\"></div>\n                    </div>\n                    <div class=\"ms-3\">\n                        <h6 class=\"mb-0\">{{ this.getUser.username }}</h6>\n                        <div v-if=\"this.getUser.roleId === 1\"><span><b>Admin</b></span></div>\n                        <div v-else-if=\"this.getUser.roleId === 2\"><span><b>Registrar</b></span></div>\n                        <div v-else><span><b>User</b></span></div>\n            \n                    </div>\n                </div>\n                <div class=\"navbar-nav w-100\">\n                    <div  v-for=\"priv in privs\" :key=\"priv.id\">\n                        <router-link :to=\"{name: priv.routeName}\" class=\"nav-item nav-link\" active-class=\"active\"><i :class=\"priv.icon\"></i>{{ priv.name }}</router-link>\n                    </div>\n                    \n<!--                      \n                    <router-link to=\"/myrecords\" class=\"nav-item nav-link\" active-class=\"active\"><i class=\"fa fa-keyboard me-2\"></i>My Records</router-link>\n                    <router-link to=\"/allrecords\" class=\"nav-item nav-link\" active-class=\"active\"><i class=\"fa fa-table me-2\"></i>All Records</router-link>\n                    <router-link to=\"/attendanceview\" class=\"nav-item nav-link\" active-class=\"active\"><i class=\"fa fa-plus-square me-2\"></i>Daily Attendance</router-link>\n                    <router-link to=\"/admin/userlist\" class=\"nav-item nav-link\" active-class=\"active\"><i class=\"fa fa-users me-2\"></i>Users</router-link> -->\n                    <a class=\"nav-item nav-link\" active-class=\"active\"  data-bs-toggle=\"offcanvas\" data-bs-target=\"#offcanvasRight\" aria-controls=\"offcanvasRight\" style=\"cursor: pointer;\"><i class=\"fa fa-user me-2\"></i>profile</a>    \n                    <!--right side canvas for profile-->\n                    \n                    <div class=\"offcanvas offcanvas-end\" tabindex=\"-1\" id=\"offcanvasRight\" aria-labelledby=\"offcanvasRightLabel\">\n                        <div class=\"offcanvas-header mt-4\">\n                            <h5 class=\"offcanvas-title\" id=\"offcanvasRightLabel\">Profile</h5>\n                            <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"offcanvas\" aria-label=\"Close\"></button>\n                        </div>\n                        <div class=\"offcanvas-body\">\n                            <h3 class=\"text-primary\">Profile settings</h3>\n                            <div class=\"container\">\n                                <form class=\"bg-light rounded h-100 p-4\" @submit.prevent=\"updateProfile\">\n\n                                    <!--Form  -->\n                                        <div class=\"mb-2 mt-0\">\n                                            <label for=\"username\" class=\"form-label\">Username</label>\n                                            <input type=\"text\" class=\"form-control\" id=\"username\" v-model=\"username\">\n                                        </div>\n                                        <div class=\"mb-2\">\n                                            <label for=\"email\" class=\"form-label\">Email</label>\n                                            <input type=\"email\" class=\"form-control\" id=\"email\" v-model=\"email\">\n                                        </div>\n                                        <div class=\"mb-2\">\n                                            <label for=\"phone\" class=\"form-label\">Phone</label>\n                                            <input type=\"number\" class=\"form-control\" id=\"phone\" v-model=\"phone\">\n                                        </div>\n                                        <div>\n                                            <label for=\"pwd\">Password</label>\n                                        </div>\n                                        <div class=\"input-group mb-3\">\n                                            <input type=\"password\" id=\"pwd\" class=\"form-control\" placeholder=\"please enter your password\" v-model=\"password\" required/>\n                                            <button class=\"btn btn-outline-secondary\" type=\"button\" id=\"button-addon2\" @click=\"viewPassword\"><i class=\"bi bi-eye-fill\" v-if=\"viewpwd\"></i><i class=\"bi bi-eye-slash\" v-if=\"!viewpwd\"></i></button>\n                                        </div>\n                                    \n                                        <div class=\"mb-3\">\n                                            <label for=\"account\">Account Type</label>\n                                            <select class=\"form-select mb-3\" aria-label=\"Default select example\"  id=\"account\" disabled v-model=\"account\">\n                                                <option selected>Admin</option>\n                                                <option>User</option>\n                                                <option>Registrar</option>\n                                            </select>\n\n                                        </div>\n                                        <div class=\"mb-3\">\n                                            <label for=\"formFile\" class=\"form-label\">Profile Avatar</label>\n                                            <div class=\"\" style=\"display: flex; flex-direction: row; width: 100%;\">\n                                                <input class=\"form-control\" type=\"file\" id=\"formFile\" v-on:change=\"profileChanged\">\n                                                <img v-if=\"profileImage\" class=\"rounded-circle me-lg-2\" :src=\"require('./assets/'+ profileAvatar)\" alt=\"\" style=\"width: 40px; height: 40px;\">\n                                            </div>\n                                            \n                                        </div>\n                                        <div class=\"mb-3\" style=\"width: 100%;\">\n                                            <button class=\"btn btn-primary\" style=\"width: 100%;\">Save</button>\n                                        </div>\n                                        \n\n                                        <!--End of form-->\n                                        \n                                    \n                                </form>\n                            </div>\n                            \n                        </div>\n                    </div>\n\n\n\n\n\n\n\n\n                    <!--end of profile-->\n                \n                </div>\n            </nav>\n        </div>\n        <!-- Sidebar End -->\n\n\n        <!-- Content Start -->\n        <div class=\"content\">\n            <!-- Navbar Start -->\n            <nav class=\"navbar navbar-expand bg-light navbar-light sticky-top px-4 py-0\">\n                <a href=\"index.html\" class=\"navbar-brand d-flex d-lg-none me-4\">\n                    <h2 class=\"text-primary mb-0\"><span class=\"badge bg-primary\">A</span></h2>\n                </a>\n                <a href=\"#\" class=\"sidebar-toggler flex-shrink-0\" @click=\"toggle\">\n                    <i class=\"fa fa-bars\"></i>\n                </a>\n                <!-- <form class=\"d-none d-md-flex ms-4\">\n                    <input class=\"form-control border-0\" type=\"search\" placeholder=\"Search\">\n                </form> -->\n                <div class=\"navbar-nav align-items-center ms-auto\">\n                    <div class=\"time-box\">\n                      <Time/>\n                        \n                    </div>\n                    <div class=\"nav-item dropdown\">\n                        <a href=\"#\" class=\"nav-link dropdown-toggle\" data-bs-toggle=\"dropdown\">\n                            <i class=\"fa fa-bell me-lg-2\"></i>\n                            <span class=\"d-none d-lg-inline-flex\">Notification</span>\n                        </a>\n                        <div class=\"dropdown-menu dropdown-menu-end bg-light border-0 rounded-0 rounded-bottom m-0\">\n                            <a href=\"#\" class=\"dropdown-item\">\n                                <h6 class=\"fw-normal mb-0\">Profile updated</h6>\n                                <small>15 minutes ago</small>\n                            </a>\n                            <hr class=\"dropdown-divider\">\n                            <a href=\"#\" class=\"dropdown-item\">\n                                <h6 class=\"fw-normal mb-0\">New user added</h6>\n                                <small>15 minutes ago</small>\n                            </a>\n                        </div>\n                    </div>\n                    <div class=\"nav-item dropdown\">\n                        <a href=\"#\" class=\"nav-link dropdown-toggle\" data-bs-toggle=\"dropdown\">\n                            <img class=\"rounded-circle me-lg-2\" src=\"./assets/profile2.jpg\" alt=\"\" style=\"width: 40px; height: 40px;\">\n                            <span class=\"d-none d-lg-inline-flex\">{{ this.getUser.username }}</span>\n                        </a>\n                        <div class=\"dropdown-menu dropdown-menu-end bg-light border-0 rounded-0 rounded-bottom m-0\">\n                            <a href=\"#\" class=\"dropdown-item\" data-bs-toggle=\"offcanvas\" data-bs-target=\"#offcanvasRight\" aria-controls=\"offcanvasRight\">My Profile  <i class=\"fa fa-user\" aria-hidden=\"true\"></i></a>\n                            <a href=\"#\" class=\"dropdown-item\"  @click=\"logout\">Sign Out     <i class=\"fa fa-power-off\"></i></a>\n                        </div>\n                    </div>\n                </div>\n            </nav>\n            <!-- Navbar End -->\n\n\n           <router-view/>\n\n            <!-- Footer Start -->\n            <div class=\"container-fluid pt-4 px-4\">\n                <div class=\"bg-light rounded-top p-4\">\n                    <div class=\"row\">\n                        <div class=\"col-12 col-sm-6 text-center text-sm-start\">\n                            &copy; <a href=\"#\">AICS-ATTEND</a>, All Right Reserved. \n                        </div>\n                    </div>\n                </div>\n            </div>\n            <!-- Footer End -->\n        </div>\n        <!-- Content End -->\n\n\n        <!-- Back to Top -->\n        <a href=\"#\" class=\"btn btn-lg btn-primary btn-lg-square back-to-top\"><i class=\"bi bi-arrow-up\"></i></a>\n    </div>\n  <div v-else>\n    <div v-if=\"!isLoaded\" id=\"spinner\" class=\"show bg-white position-fixed translate-middle w-100 vh-100 top-50 start-50 d-flex align-items-center justify-content-center\">\n            <div class=\"spinner-border text-primary\" style=\"width: 3rem; height: 3rem;\" role=\"status\">\n                <span class=\"sr-only\">Loading...</span>\n            </div>\n    </div>\n\n    <router-view/>\n    \n    </div>\n</template>\n<script>\n\nimport Time from '../src/components/Time.vue'\nimport $ from 'jquery';\nimport SidebarMixin from './SidebarMixin.js'\nimport { mapActions, mapGetters } from 'vuex';\nimport { makeRequest } from './axios';\n\n\nexport default {\n  mixins: [SidebarMixin],\n  components:{Time},\n  data(){\n    return{\n        viewpwd:false,\n        isLoaded:false,\n        isopen:false,\n        profileImage:true,\n        username:'John',\n        email:'john@gmail.com',\n        phone:'672369066',\n        password:'John1234#',\n        account:'Admin',\n        profileAvatar:'profile2.jpg',\n        getUser:[],\n        privs:[]\n        \n    }\n  },\n  mounted(){\n    \n    this.isLoaded=true\n    if(SidebarMixin.ShouldShowSidebar){\n    //spinner.classList.remove('show')\n    // const script = document.createElement('script')\n    // script.src = require('./assets/loaded.js')\n    // document.head.appendChild(script)\n\n    //profile check\n    if(!this.profileAvatar){\n        this.profileImage=false\n    }\n    console.log('mounted')\n    //console.log(this.getUId)\n    //console.log(this.getAuthStatus)\n    //fetch user personal data\n\n    //fetch user role\n\n   \n\n    //check if user is authenticated\n}\n\n  },\n  beforeCreate(){\n    console.log(\"before create\")\n  },\n  beforeMount(){\n    console.log(\"before mount\")\n  },\n  created() {\n    // if (this.isAuthenticated) {\n    //   this.fetchprivs();\n   if(SidebarMixin.ShouldShowSidebar){\n    if(this.isAuthenticated){\n        this.fetch_user_data()\n    }else{\n        this.$router.push('/')\n    }\n   }\n    \n    \n    },\n  computed:{\n    ...mapGetters(['getUId','getAuthStatus']),\n    \n\n    isAuthenticated(){\n        \n        return this.getAuthStatus\n    }\n    \n\n  },\n  methods:{\n    ...mapActions(['setUId','setAuth']),\n    toggle(){\n        $('.sidebar, .content').toggleClass(\"open\");\n        return false;\n    },\n    profileChanged(event) {\n        this.profileImage=false\n      this.profileAvatar = event.target.files[0];\n    },\n    viewPassword(){\n    const pwd = document.getElementById('pwd')\n      if(pwd.type==='password'){\n        pwd.type='text'\n      }\n      else{\n        pwd.type='password'\n      }\n      this.viewpwd=!this.viewpwd\n\n    },\n    logout(){\n        makeRequest.get('/auth/logout')\n        .then(res=>{\n            if(!res.data.Error){\n                //set data in the store null\n                const user_id = null\n                const authStatus = false\n                this.setUId(user_id)\n                this.setAuth(authStatus)\n                this.$router.push('/');\n                console.log(\"logged out\")\n            }else{\n                this.$toast.error(res.data.Error)\n            }\n\n        })\n        .catch(err=>{\n            console.log(err);\n            this.$toast.error(\"Network error, please try again\")\n        })\n        \n    },\n    fetch_user_data(){\n\n        makeRequest.get('/auth/')\n        .then(res=>{\n            if(!res.data.Error){\n                this.getUser = res.data\n                console.log(res.data)\n                this.fetchprivs(res.data.roleId)\n\n            }else{\n                console.log(res.data.Eror)\n            }\n\n        })\n        .catch(err=>{\n            console.log(err)\n            this.$toast(\"could not load user details\")\n        })\n\n    },\n    \n    fetchprivs(role_id){\n\n        let user_role = role_id;\n            makeRequest.get(\"/roles/getprivs/\"+user_role)\n            .then(result=>{\n                if(!result.data.Error){\n                    console.log(result.data.Privileges)\n                    this.privs=result.data.Privileges\n\n                }else{\n                    this.$toast.error(this.data.error)\n                }\n\n            })\n            .catch(error=>{\n                this.$toast.error(\"Network error, try agin\")\n                console.log(error)\n            })\n\n    }\n    \n  }\n\n}\n\n\n</script>\n\n<style>\nbody{\n    background: white;\n}\n   \n   \n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n}\n\n.alert-danger {\n  animation: fadeIn 2s forwards;\n}\n@keyframes fadeIn {\n  0% {\n    opacity: 0;\n  }\n  100% {\n    opacity: 1;\n    display: inline;\n  }\n}\n\nnav {\n  padding: 30px;\n}\n\nnav a {\n  font-weight: bold;\n  color: #2c3e50;\n}\n\nnav a.router-link-exact-active {\n  color: #42b983;\n}\n:root {\n        --primary: #009CFF;\n        --light: #F3F6F9;\n        --dark: #191C24;\n    }\n    \n    .back-to-top {\n        position: fixed;\n        display: none;\n        right: 45px;\n        bottom: 45px;\n        z-index: 99;\n    }\n    \n    \n    /*** Spinner ***/\n    #spinner {\n        opacity: 0;\n        visibility: hidden;\n        transition: opacity .5s ease-out, visibility 0s linear .5s;\n        z-index: 99999;\n    }\n    \n    #spinner.show {\n        transition: opacity .5s ease-out, visibility 0s linear 0s;\n        visibility: visible;\n        opacity: 1;\n    }\n    \n    \n    /*** Button ***/\n    .btn {\n        transition: .5s;\n    }\n    \n    .btn.btn-primary {\n        color: #FFFFFF;\n    }\n    \n    .btn-square {\n        width: 38px;\n        height: 38px;\n    }\n    \n    .btn-sm-square {\n        width: 32px;\n        height: 32px;\n    }\n    \n    .btn-lg-square {\n        width: 48px;\n        height: 48px;\n    }\n    \n    .btn-square,\n    .btn-sm-square,\n    .btn-lg-square {\n        padding: 0;\n        display: inline-flex;\n        align-items: center;\n        justify-content: center;\n        font-weight: normal;\n        border-radius: 50px;\n    }\n    \n    \n    /*** Layout ***/\n    .sidebar {\n        position: fixed;\n        top: 0;\n        left: 0;\n        bottom: 0;\n        width: 250px;\n        height: 100vh;\n        overflow-y: auto;\n        background: var(--light);\n        transition: 0.5s;\n        z-index: 999;\n    }\n    \n    .content {\n        margin-left: 250px;\n        min-height: 100vh;\n        background: #FFFFFF;\n        transition: 0.5s;\n    }\n    \n    @media (min-width: 992px) {\n        .sidebar {\n            margin-left: 0;\n        }\n    \n        .sidebar.open {\n            margin-left: -250px;\n        }\n    \n        .content {\n            width: calc(100% - 250px);\n        }\n    \n        .content.open {\n            width: 100%;\n            margin-left: 0;\n        }\n    }\n    \n    @media (max-width: 991.98px) {\n        .sidebar {\n            margin-left: -250px;\n        }\n    \n        .sidebar.open {\n            margin-left: 0;\n        }\n   