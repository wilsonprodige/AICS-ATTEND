{"ast":null,"code":"import LoginImg from '../components/LoginImg.vue';\nexport default {\n  name: 'RegisterView',\n  data() {\n    return {\n      showAlert: false,\n      viewpwd: false,\n      viewconpwd: false,\n      name: '',\n      username: '',\n      email: '',\n      password: '',\n      confirmpassword: '',\n      acceptterms: false,\n      nameError: '',\n      passwordError: '',\n      usernameError: '',\n      emailError: '',\n      confirmpassError: '',\n      emailCorrect: false,\n      passwordCorrect: false\n    };\n  },\n  methods: {\n    /* eslint-disable */\n\n    isNameValid() {\n      let minsize = 2;\n      let maxsize = 60;\n      let hasValidcharacters = /^[a-zA-Z'-]+$/.test(this.name);\n      return this.name.length > minsize && this.name.length < maxsize && hasValidcharacters;\n    },\n    isUsernameValid() {\n      let minsize = 5;\n      let hasValidcharacters = /^[a-zA-Z'-]+$/.test(this.username);\n      return this.username.length >= minsize && hasValidcharacters;\n    },\n    isEmailValid(value) {\n      let Regex = /^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$/;\n      return Regex.test(value);\n    },\n    isPasswordValid(value) {\n      let minLength = 8;\n      let hasUpperCase = /[A-Z]/.test(value);\n      const hasLowerCase = /[a-z]/.test(value);\n      const hasNumber = /\\d/.test(value);\n      const hasSpecialChar = /[!@#$%^&*()_+\\-=\\[\\]{};':\"\\\\|,.<>\\/?]/.test(value);\n      return value.length >= minLength && hasUpperCase && hasLowerCase && hasNumber && hasSpecialChar;\n    },\n    viewPassword() {\n      if (pwd.type === 'password') {\n        pwd.type = 'text';\n      } else {\n        pwd.type = 'password';\n      }\n      this.viewpwd = !this.viewpwd;\n    },\n    viewconPassword() {\n      if (conpwd.type === 'password') {\n        conpwd.type = 'text';\n      } else {\n        conpwd.type = 'password';\n      }\n      this.viewconpwd = !this.viewconpwd;\n    },\n    register() {\n      //check if password and email are valid\n      if (this.isEmailValid(this.email)) {\n        if (this.isPasswordValid(this.password)) {\n          //check complete, validation at server level\n          //console.log(this.email,this.password,this.remember)\n          if (this.isNameValid()) {\n            //console.log(this.isNameValid())\n            //onsole.log('here name is valid')\n\n            if (this.password === this.confirmpassword) {\n              //console.log('passwords are same')\n\n              if (this.isUsernameValid()) {\n                //console.log('username valid')\n                //all inputs check\n                //check if username does not exist and save if its the case \n\n                console.log(this.name, this.username, this.email, this.password);\n              } else {\n                this.usernameError = 'Username must be atleast 5 characters and should not contain special characters';\n              }\n            } else {\n              this.confirmpassError = 'Confirm password does not match password';\n            }\n          } else {\n            if (!this.nameError) {\n              this.nameError = 'name must be less than 60 characters,greater than 2, and should not contain special characters';\n              console.log(this.nameError);\n            }\n          }\n        } else {\n          this.passwordError = 'Invalid password,password must be atleast 8 characters, contain lower case,uppercase and special characters';\n        }\n      } else {\n        this.emailError = 'Invalid email';\n      }\n    },\n    deleteError() {\n      this.emailError = '';\n      this.passwordError = '';\n      this.nameError = '';\n      this.confirmpassError = '';\n      this.usernameError = '';\n    }\n  },\n  components: {\n    LoginImg\n  },\n  watch: {\n    email(value) {\n      if (this.isEmailValid(value)) {\n        this.emailCorrect = true;\n      } else {\n        this.emailCorrect = false;\n      }\n    },\n    password(value) {\n      if (this.isPasswordValid(value)) {\n        this.passwordCorrect = true;\n      } else {\n        this.passwordCorrect = false;\n      }\n    },\n    acceptterms(value) {\n      if (value) {\n        //console.log(value)\n        submitbtn.classList.remove('disabled');\n      } else {\n        //console.log(value)\n        submitbtn.classList.add('disabled');\n      }\n    }\n  },\n  computed() {\n    console.log(this.email);\n  },\n  mounted() {}\n};","map":{"version":3,"names":["LoginImg","name","data","showAlert","viewpwd","viewconpwd","username","email","password","confirmpassword","acceptterms","nameError","passwordError","usernameError","emailError","confirmpassError","emailCorrect","passwordCorrect","methods","isNameValid","minsize","maxsize","hasValidcharacters","test","length","isUsernameValid","isEmailValid","value","Regex","isPasswordValid","minLength","hasUpperCase","hasLowerCase","hasNumber","hasSpecialChar","viewPassword","pwd","type","viewconPassword","conpwd","register","console","log","deleteError","components","watch","submitbtn","classList","remove","add","computed","mounted"],"sources":["/Users/hardpro/Desktop/Vue JS/aics-attend/src/views/Register.vue"],"sourcesContent":["<template>\n  <div class=\"container py-5 px-5\">\n    \n\n    <div class=\"container-fluid d-flex align-items-center rounded shadow-lg\">\n      <div class=\"box myboxes\">\n          <LoginImg :src=\"require('../assets/register.png')\"/>\n      </div>\n      <div class=\"box myboxes\">\n        <!---alert-->\n        <div>\n            <div class=\"alert alert-danger overflow-scroll\" id=\"alert\" role=\"alert\"  v-show=\"passwordError||emailError||nameError||confirmpassError||usernameError\" @transitionend=\"deleteError\">\n              {{ passwordError }} {{ emailError }}{{ nameError }} {{ confirmpassError }}{{ usernameError }}\n              <button type=\"button\" class=\"btn-close\" @click=\"deleteError\"></button>\n            </div>\n        </div>\n\n      <!--End of alert-->\n\n        <h1 class=\"mt-0\">Sign Up...</h1>\n        <form @submit.prevent=\"register\">\n          <div class=\"mb-3\">\n            <label for=\"fullname\">Full name</label>\n            <input type=\"text\" id=\"fullname\" class=\"form-control\" placeholder=\"please enter your full namee..\" v-model=\"name\" required/>\n          </div>\n\n          <div class=\"mb-3\">\n            <label for=\"username\">Username</label>\n            <input type=\"text\" id=\"username\" class=\"form-control\" placeholder=\"please enter your username..\" v-model=\"username\" required/>\n          </div>\n\n\n          <div class=\"mb-3\">\n            <label for=\"email\">Email</label>\n            <input type=\"email\" id=\"email\" class=\"form-control\" placeholder=\"please enter your email..\" v-model=\"email\" required/>\n          </div>\n          <div class=\"error\" v-if=\"emailCorrect\"><p class=\"text-success\">Valid Email</p></div>\n  \n\n\n          <div>\n            <label for=\"pwd\">Password</label>\n          </div>\n          <div class=\"input-group mb-3\">\n            <input type=\"password\" id=\"pwd\" class=\"form-control\" placeholder=\"please enter your password\" v-model=\"password\" required/>\n            <button class=\"btn btn-outline-secondary\" type=\"button\" id=\"button-addon2\" @click=\"viewPassword\"><i class=\"bi bi-eye-fill\" v-if=\"viewpwd\"></i><i class=\"bi bi-eye-slash\" v-if=\"!viewpwd\"></i></button>\n          </div>\n          <div class=\"error\" v-if=\"passwordCorrect\"><p class=\"text-success\">Valid Password</p></div>\n\n          <div>\n            <label for=\"conpwd\">Confirm Password</label>\n          </div>\n          <div class=\"input-group mb-3\">\n            <input type=\"password\" id=\"conpwd\" class=\"form-control\" placeholder=\"confirm your password\" v-model=\"confirmpassword\" required/>\n            <button class=\"btn btn-outline-secondary\" type=\"button\" id=\"button-addon3\" @click=\"viewconPassword\"><i class=\"bi bi-eye-fill\" v-if=\"viewconpwd\"></i><i class=\"bi bi-eye-slash\" v-if=\"!viewconpwd\"></i></button>\n          </div>\n          \n          \n\n          <div class=\"form-check mb-3\">\n            <input class=\"form-check-input\" type=\"checkbox\" value=\"\" id=\"flexCheckDefault\" v-model=\"acceptterms\">\n            <label class=\"la form-check-label\" for=\"flexCheckDefault\">\n              Accept the <a href=\"\">terms and conditions</a>\n            </label><br>\n            \n          </div>\n          \n            <p>Already have an account<router-link to=\"/\">Sign-In</router-link></p>\n\n          <div class=\"input-group\">\n            <button id=\"submitbtn\" type=\"submit\" class=\"btn btn-primary btn-md disabled\">Sign Up</button>\n\n          </div>\n            \n\n          \n          \n  \n            \n\n          \n          \n        </form>\n\n\n\n\n      \n      </div>\n    </div>\n\n\n    \n\n  </div>\n       \n      \n\n\n     \n</template>\n\n<script>\n\nimport LoginImg from '../components/LoginImg.vue'\n\nexport default {\n    name:'RegisterView',\n    data(){\n    return{\n      showAlert:false,\n      viewpwd:false,\n      viewconpwd:false,\n      name:'',\n      username:'',\n      email:'',\n      password:'',\n      confirmpassword:'',\n      acceptterms:false,\n      nameError:'',\n      passwordError:'',\n      usernameError:'',\n      emailError:'',\n      confirmpassError:'',\n      emailCorrect:false,\n      passwordCorrect:false\n\n\n\n    }\n  },\n\n  methods:{\n    /* eslint-disable */ \n\n    isNameValid(){\n        let minsize=2\n        let maxsize=60\n        let hasValidcharacters = /^[a-zA-Z'-]+$/.test(this.name)\n\n\n        return this.name.length>minsize && this.name.length < maxsize && hasValidcharacters\n\n    },\n    isUsernameValid(){\n\n      let minsize=5\n      let hasValidcharacters = /^[a-zA-Z'-]+$/.test(this.username)\n\n      return this.username.length >= minsize && hasValidcharacters\n    },\n    isEmailValid(value){\n      let  Regex = /^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$/;\n      return Regex.test(value)\n\n\n    },\n    isPasswordValid(value){\n      let minLength = 8;\n      let hasUpperCase = /[A-Z]/.test(value);\n      const hasLowerCase = /[a-z]/.test(value);\n      const hasNumber = /\\d/.test(value);\n      const hasSpecialChar = /[!@#$%^&*()_+\\-=\\[\\]{};':\"\\\\|,.<>\\/?]/.test(value);\n      return value.length >= minLength && hasUpperCase && hasLowerCase && hasNumber && hasSpecialChar;\n          \n\n\n    },\n    viewPassword(){\n      if(pwd.type==='password'){\n        pwd.type='text'\n      }\n      else{\n        pwd.type='password'\n      }\n      this.viewpwd=!this.viewpwd\n\n    },\n    viewconPassword(){\n      if(conpwd.type==='password'){\n        conpwd.type='text'\n      }\n      else{\n        conpwd.type='password'\n      }\n      this.viewconpwd=!this.viewconpwd\n\n    },\n\n    register(){\n      //check if password and email are valid\n      if(this.isEmailValid(this.email)){\n        if(this.isPasswordValid(this.password)){\n          //check complete, validation at server level\n          //console.log(this.email,this.password,this.remember)\n          if(this.isNameValid()){\n            //console.log(this.isNameValid())\n            //onsole.log('here name is valid')\n\n            if(this.password===this.confirmpassword){\n              //console.log('passwords are same')\n\n              if(this.isUsernameValid()){\n                //console.log('username valid')\n                //all inputs check\n                //check if username does not exist and save if its the case \n\n                console.log(this.name,this.username,this.email,this.password)\n\n              }else{\n                this.usernameError='Username must be atleast 5 characters and should not contain special characters'\n              }\n            }\n            else{\n              this.confirmpassError='Confirm password does not match password'\n\n            }\n\n          }else{\n            if(!this.nameError){\n                this.nameError='name must be less than 60 characters,greater than 2, and should not contain special characters'\n                console.log(this.nameError)\n                \n            }\n          }\n\n        }\n        else{\n          \n          this.passwordError='Invalid password,password must be atleast 8 characters, contain lower case,uppercase and special characters'\n          \n          \n          \n\n        }\n\n      }\n      else{\n        this.emailError='Invalid email'\n\n        \n      }\n\n    },\n    deleteError(){\n        this.emailError=''\n        this.passwordError=''\n        this.nameError=''\n        this.confirmpassError=''\n        this.usernameError=''\n        \n\n    }\n\n\n    \n  },\n  components: {\n    LoginImg\n    \n  \n  },\n  watch:{\n    email(value){\n      if(this.isEmailValid(value)){\n        this.emailCorrect=true\n      }\n      else{\n        this.emailCorrect=false\n      }\n    },\n\n    password(value){\n      if(this.isPasswordValid(value)){\n        this.passwordCorrect=true\n      }\n      else{\n        this.passwordCorrect=false\n      }\n\n    },\n    acceptterms(value){\n        if(value){\n            //console.log(value)\n            submitbtn.classList.remove('disabled')\n        }\n        else{\n            //console.log(value)\n            submitbtn.classList.add('disabled')\n\n        }\n    }\n\n  },\n  computed(){\n    console.log(this.email)\n\n  },\n  mounted(){\n\n\n  }\n}\n\n\n</script>\n\n<style scoped>\n\n@import url(\"https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.5/font/bootstrap-icons.css\");\n\n\nbody{\n  height: 100vh;\n  background: url('../assets/back.svg');\n  background-size: cover;\n}\n\n.container-fluid{\n\nborder-radius: 15px;\nbackground: white;\nmargin-top: 20%;\n\n}\n.myboxes{\n  padding: 15px;\n  width: 50%;\n \n\n}\nlabel,button,p{\n    float: left;\n  }\n  form{\n    display: flex;\n    flex-direction: column;\n  }\n.btn-md{\n  width: 100px;\n}\n\n\n@media (max-width: 768px) {\n  .container-fluid {\n    flex-direction: column;\n    \n  }\n  .myboxes{\n    padding: 25px;\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    width: 100%;\n   \n  }\n  body{\n    height: 100%;\n  }\n  .main-container{\n    margin-top: auto;\n    \n  }\n  \n}\n\n\n\n\n\n\n</style>"],"mappings":"AAwGA,OAAOA,QAAO,MAAO,4BAA2B;AAEhD,eAAe;EACXC,IAAI,EAAC,cAAc;EACnBC,IAAIA,CAAA,EAAE;IACN,OAAM;MACJC,SAAS,EAAC,KAAK;MACfC,OAAO,EAAC,KAAK;MACbC,UAAU,EAAC,KAAK;MAChBJ,IAAI,EAAC,EAAE;MACPK,QAAQ,EAAC,EAAE;MACXC,KAAK,EAAC,EAAE;MACRC,QAAQ,EAAC,EAAE;MACXC,eAAe,EAAC,EAAE;MAClBC,WAAW,EAAC,KAAK;MACjBC,SAAS,EAAC,EAAE;MACZC,aAAa,EAAC,EAAE;MAChBC,aAAa,EAAC,EAAE;MAChBC,UAAU,EAAC,EAAE;MACbC,gBAAgB,EAAC,EAAE;MACnBC,YAAY,EAAC,KAAK;MAClBC,eAAe,EAAC;IAIlB;EACF,CAAC;EAEDC,OAAO,EAAC;IACN;;IAEAC,WAAWA,CAAA,EAAE;MACT,IAAIC,OAAO,GAAC;MACZ,IAAIC,OAAO,GAAC,EAAC;MACb,IAAIC,kBAAiB,GAAI,eAAe,CAACC,IAAI,CAAC,IAAI,CAACtB,IAAI;MAGvD,OAAO,IAAI,CAACA,IAAI,CAACuB,MAAM,GAACJ,OAAM,IAAK,IAAI,CAACnB,IAAI,CAACuB,MAAK,GAAIH,OAAM,IAAKC,kBAAiB;IAEtF,CAAC;IACDG,eAAeA,CAAA,EAAE;MAEf,IAAIL,OAAO,GAAC;MACZ,IAAIE,kBAAiB,GAAI,eAAe,CAACC,IAAI,CAAC,IAAI,CAACjB,QAAQ;MAE3D,OAAO,IAAI,CAACA,QAAQ,CAACkB,MAAK,IAAKJ,OAAM,IAAKE,kBAAiB;IAC7D,CAAC;IACDI,YAAYA,CAACC,KAAK,EAAC;MACjB,IAAKC,KAAI,GAAI,4BAA4B;MACzC,OAAOA,KAAK,CAACL,IAAI,CAACI,KAAK;IAGzB,CAAC;IACDE,eAAeA,CAACF,KAAK,EAAC;MACpB,IAAIG,SAAQ,GAAI,CAAC;MACjB,IAAIC,YAAW,GAAI,OAAO,CAACR,IAAI,CAACI,KAAK,CAAC;MACtC,MAAMK,YAAW,GAAI,OAAO,CAACT,IAAI,CAACI,KAAK,CAAC;MACxC,MAAMM,SAAQ,GAAI,IAAI,CAACV,IAAI,CAACI,KAAK,CAAC;MAClC,MAAMO,cAAa,GAAI,uCAAuC,CAACX,IAAI,CAACI,KAAK,CAAC;MAC1E,OAAOA,KAAK,CAACH,MAAK,IAAKM,SAAQ,IAAKC,YAAW,IAAKC,YAAW,IAAKC,SAAQ,IAAKC,cAAc;IAIjG,CAAC;IACDC,YAAYA,CAAA,EAAE;MACZ,IAAGC,GAAG,CAACC,IAAI,KAAG,UAAU,EAAC;QACvBD,GAAG,CAACC,IAAI,GAAC,MAAK;MAChB,OACI;QACFD,GAAG,CAACC,IAAI,GAAC,UAAS;MACpB;MACA,IAAI,CAACjC,OAAO,GAAC,CAAC,IAAI,CAACA,OAAM;IAE3